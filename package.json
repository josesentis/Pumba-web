    "three": "^0.140.1",
    "-": "0.0.1",
    "dotenv": "^16.0.0",
    "fs": "^0.0.1-security",
    "g": "^2.0.1",
    "lottie-web": "^5.9.2",
    "netlify-lambda": "^2.0.15",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.9"
  }
}
